    xmlns:app="http://schemas.android.com/tools"
    xmlns:card_view="http://schemas.android.com/apk/res-auto"
    android:id="@+id/card_view"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_margin="7dp"
    app:ignore="NamespaceTypo"
    card_view:cardCornerRadius="4dp"
    card_view:cardElevation="4dp">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:padding="@dimen/space_mid10">


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:paddingLeft="@dimen/space_mid10"
            android:paddingRight="@dimen/space_mid10">

            <TextView
                android:layout_width="104dp"
                android:layout_height="wrap_content"
                android:layout_weight=".3"
                android:text="Comp No"
                android:textColor="@color/header_color"
                android:textSize="@dimen/size_small14" />

            <TextView
                android:id="@+id/cmp_no_value"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"

                android:textSize="@dimen/size_small14" />

        </LinearLayout>

        <LinearLayout
            android:id="@+id/lin1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:paddingLeft="@dimen/space_mid10"
            android:paddingRight="@dimen/space_mid10">

            <TextView
                android:layout_width="106dp"
                android:layout_height="wrap_content"
                android:layout_weight=".3"
                android:text="Comp Date"
                android:textColor="@color/header_color"
                android:textSize="@dimen/size_small14" />

            <TextView
                android:id="@+id/cmp_date_value"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:textSize="@dimen/size_small14" />


        </LinearLayout>

        <LinearLayout
            android:id="@+id/lin2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:paddingLeft="@dimen/space_mid10"
            android:paddingRight="@dimen/space_mid10">

            <TextView
                android:id="@+id/name"
                android:layout_width="106dp"
                android:layout_height="wrap_content"
                android:layout_weight=".3"
                android:text="Subordinate Name"
                android:textColor="@color/header_color"
                android:textSize="@dimen/size_small14" />

            <TextView
                android:id="@+id/name_value"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:textSize="@dimen/size_small14" />


        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:paddingLeft="@dimen/space_mid10"
            android:paddingRight="@dimen/space_mid10">

            <TextView

                android:layout_width="103dp"
                android:layout_height="wrap_content"
                android:layout_weight=".3"
                android:text="Subordinate Mobile"
                android:textColor="@color/header_color"
                android:textSize="@dimen/size_small14" />

            <TextView
                android:id="@+id/sub_mobile_value"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:textSize="@dimen/size_small14" />

        </LinearLayout>

