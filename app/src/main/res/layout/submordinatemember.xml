                android:fontFamily="@font/open_sans_regular"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="From Date: "
                android:textSize="@dimen/_10ssp"
                android:textColor="@color/black"
                />

            <TextView
